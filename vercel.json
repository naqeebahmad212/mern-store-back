{
    "version":2,
    "builds":[ 
       {  "src":"app.js", "use":"@vercel/node"}
],
"routes":[
    {
        "src":"/(.*)",
        "dest":"/app.js",
        "method":["GET","POST","DELETE","PUT","PATCH","OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin":"https://mernstack-frontend-phi.vercel.app",
            "Access-Control-Allow-Credentials":"true"
        }
    }
]
}